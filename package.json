{
  "name": "flowstick",
  "productName": "Flowstick",
  "version": "0.1.0",
  "description": "A fast and fun XPDL editor.",
  "author": {
    "name": "Eric Butler",
    "email": "idealeric@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "run": "cross-env NODE_ENV=production electron ./",
    "run-dev": "cross-env NODE_ENV=development HOT=1 electron ./",
    "build": "cross-env NODE_ENV=production webpack",
    "build-dev": "cross-env NODE_ENV=development webpack-dev-server --port 3000 --hot --inline",
    "dev": "concurrently --kill-others \"yarn run build-dev\" \"yarn run run-dev\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "dependencies": {
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "electron": "1.4.7",
    "elm": "0.18.0",
    "elm-css": "0.6.0",
    "elm-css-webpack-loader": "2.0.2",
    "elm-webpack-loader": "3.0.6",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.11.1",
    "material-design-icons": "3.0.1",
    "rxjs": "^5.3.0",
    "style-loader": "^0.16.1",
    "webpack": "1.13.3",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "electron-debug": "1.1.0",
    "elm-hot-loader": "0.4.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-dev-server": "1.16.2"
  }
}
