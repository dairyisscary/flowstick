{
  "name": "flowstick",
  "productName": "Flowstick",
  "version": "0.1.0",
  "description": "A fast and fun XPDL editor.",
  "author": {
    "name": "Eric Butler",
    "email": "idealeric@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "run": "cross-env NODE_ENV=production electron ./",
    "run-dev": "cross-env NODE_ENV=development HOT=1 electron ./",
    "build": "cross-env NODE_ENV=production webpack",
    "build-dev": "cross-env NODE_ENV=development webpack-dev-server --port 3000 --hot --inline",
    "dev": "concurrently --kill-others \"npm run build-dev\" \"npm run run-dev\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "dependencies": {
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.1",
    "electron": "1.4.0",
    "elm": "0.17.1",
    "elm-webpack-loader": "3.0.6",
    "file-loader": "0.9.0",
    "material-design-icons": "3.0.1",
    "webpack": "1.13.2"
  },
  "devDependencies": {
    "electron-debug": "1.0.1",
    "elm-hot-loader": "0.3.4",
    "webpack-dev-middleware": "1.7.0",
    "webpack-dev-server": "1.15.2"
  }
}
